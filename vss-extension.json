{
  "manifestVersion": 1,
  "id": "library-enhancer",
  "name": "Library Enhancer",
  "version": "2.1.1",
  "publisher": "milvasoft",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Enhanced Library/Variable Groups management with hierarchical grouping. Open-source and privacy-focused - runs entirely in your browser, no external data transmission.",
  "categories": ["Azure Pipelines"],
  "tags": [
    "variable groups",
    "library",
    "pipelines",
    "organization",
    "hierarchy",
    "open-source",
    "privacy"
  ],
  "repository": {
    "type": "git",
    "uri": "https://github.com/Milvasoft/azuredevops-library-enhancer"
  },
  "links": {
    "home": {
      "uri": "https://github.com/Milvasoft/azuredevops-library-enhancer"
    },
    "repository": {
      "uri": "https://github.com/Milvasoft/azuredevops-library-enhancer"
    },
    "issues": {
      "uri": "https://github.com/Milvasoft/azuredevops-library-enhancer/issues"
    },
    "support": {
      "uri": "https://github.com/Milvasoft/azuredevops-library-enhancer/issues"
    }
  },
  "scopes": [
    "vso.variablegroups_read"
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "contributions": [
    {
      "id": "enhanced-library-hub",
      "type": "ms.vss-web.hub",
      "description": "Enhanced Library Hub with hierarchical variable groups",
      "targets": [
        "ms.vss-build-web.build-release-hub-group"
      ],
      "properties": {
        "name": "Library (Enhanced)",
        "order": 31,
        "uri": "dist/library-hub.html"
      }
    }
  ],
  "files": [
    {
      "path": "dist",
      "addressable": true,
      "packagePath": "dist"
    },
    {
      "path": "images",
      "addressable": true,
      "packagePath": "images"
    }
  ],
  "ContentType": {
    ".woff2": "font/woff2",
    ".woff": "font/woff"
  }
}
